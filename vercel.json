{
  "version": 2,
  "builds": [
  {
  "src": "frontend/**/",
  "use": "@vercel/static"
  },
  {
  "src": "api/backend.js",
  "use": "@vercel/node"
  },
  {
  "src": "functions/index.js",
  "use": "@vercel/node"
  }
  ],
  "routes": [
  {
  "src": "/api/ia",
  "dest": "/api/backend.js"
  },
  {
  "src": "/api/backend/(.)",
  "dest": "/api/backend.js"
  },
  {
  "src": "/api/functions/(.)",
  "dest": "/functions/index.js"
  },
  {
  "src": "/(.)",
  "dest": "/frontend/$1"
  }
  ]
  }